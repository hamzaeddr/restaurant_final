<div class="row  m-auto py-3">
                                {% for iteme in list %}

                <div class="col-lg-4">
                    <div class="py-2">
                        <img src="img/imghamza.jpg" alt="" class="T-img">
                    </div>
                  <div class="mb-3">
                    <label for="formFileSm" class="form-label">Ajouter une image</label>
                    <input class="form-control form-control-sm" id="formFileSm" type="file">
                  </div>
                </div>
                <div class="col-lg-6" style="font-size: 13px; font-weight: 700;margin: -20px 25px ;">
                
                   <p> 
                        <div class="input-group">
                            <span class="input-group-text">ID Famille</span>
                            <input type="text" name="idfamille" id="id-famille" aria-label="First name" class="form-control" value="{{iteme.IdFamilleC}}" readonly>
                        </div>
                   </p> 
                   <p>
                        <div class="input-group">
                            <span class="input-group-text">Famille</span>
                            <input type="text" name="famille" id="famille" aria-label="First name" class="form-control" value="{{iteme.FamilleC}}">
                        </div>
                   </p>  
                   <p>
                        <div class="input-group">
                            <select class="form-control" id="tfamille">
                           {% for item in lsttypef %}
                                   {% if item.TypeFamille == iteme.TypeFamille.TypeFamille %}

                                    <option  value="{{item.id}}">{{item.TypeFamille}}</option>

                                 {% endif %}
                                {% endfor %}  
                                {% for item in lsttypef %}
                                   {% if item.TypeFamille != iteme.TypeFamille.TypeFamille %}
                                  <option  value="{{item.id}}">{{item.TypeFamille}}</option>

                                  {% endif %}
                                {% endfor %}  
                               
                              </select>
                        </div>
                   </p>  
                   <p>
                        <div class="input-group">
                            <span class="input-group-text">Nom Arabe</span>
                            <input type="text" id="farabe" aria-label="First name" class="form-control" value="{{iteme.FArabeC}}">
                        </div>
                   </p>
                   <p>visible : <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault"></p>
                </div>
                
            </div>

                                {% endfor %}  
            